--enable-debug
--enable-encoders=lzma1,delta
--enable-decoders=lzma1,powerpc
--enable-match-finders=hc3,hc4
--enable-checks=crc32
--enable-assume-ram=128
--enable-sandbox=capsicum
--enable-silent-rules
--enable-unaligned-access
--with-pic=PIC
--with-aix-soname=aix
--with-gnu-ld=yes
--without-libiconv-prefix
--without-libintl-prefix
--disable-option-checking
--disable-xzdec
--disable-lzmadec
--disable-scripts
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
