--enable-debug
--enable-encoders=lzma1,delta
--enable-decoders=lzma1,x86
--enable-match-finders=hc3,hc4
--enable-checks=crc32,crc64,sha256
--enable-small
--enable-threads=posix
--enable-symbol-versions
--enable-sandbox=no
--enable-dependency-tracking
--with-pic=PIC
--with-gnu-ld
--with-gnu-ld=yes
--without-libintl-prefix
--disable-option-checking
--disable-lzmadec
--disable-lzmainfo
--disable-scripts
--disable-doc
--disable-dependency-tracking
--disable-libtool-lock
--disable-rpath
--disable-largefile
