--enable-encoders=lzma1,lzma2,x86,armthumb,sparc,arm,ia64,powerpc,delta
--enable-decoders=lzma1
--enable-match-finders=bt2,bt3
--enable-checks=crc32
--enable-threads=no
--enable-assume-ram=128
--enable-sandbox=auto
--enable-dependency-tracking
--enable-unsafe-type-punning
--enable-fast-install=no
--with-aix-soname=both
--with-gnu-ld=yes
--without-libiconv-prefix
--without-libintl-prefix
--disable-option-checking
--disable-xzdec
--disable-lzmadec
--disable-lzmainfo
--disable-lzma-links
--disable-scripts
--disable-silent-rules
--disable-dependency-tracking
--disable-nls
--disable-rpath
--disable-largefile
