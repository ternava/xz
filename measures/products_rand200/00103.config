--enable-debug
--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1,sparc
--enable-match-finders=hc4,hc3,bt4
--enable-checks=crc32
--enable-small
--enable-threads=yes
--enable-assume-ram=128
--enable-sandbox=no
--enable-dependency-tracking
--enable-unsafe-type-punning
--with-gnu-ld
--with-gnu-ld=yes
--without-libiconv-prefix
--disable-scripts
--disable-silent-rules
--disable-nls
--disable-rpath
--disable-largefile
