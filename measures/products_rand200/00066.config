--enable-debug
--enable-encoders=lzma1,armthumb
--enable-decoders=lzma1,powerpc
--enable-match-finders=bt3
--enable-checks=crc32,crc64
--enable-assume-ram=128
--enable-sandbox=capsicum
--enable-silent-rules
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-fast-install=no
--with-pic=non-PIC
--with-aix-soname=svr4
--with-gnu-ld=yes
--without-libiconv-prefix
--disable-assembler
--disable-xzdec
--disable-lzmainfo
--disable-silent-rules
--disable-libtool-lock
--disable-nls
