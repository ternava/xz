--enable-debug
--enable-encoders=lzma1,lzma2
--enable-decoders=lzma1,armthumb
--enable-match-finders=bt2
--enable-small
--enable-threads=yes
--enable-sandbox=auto
--enable-silent-rules
--enable-dependency-tracking
--enable-fast-install
--enable-fast-install=no
--with-pic=PIC
--with-aix-soname=svr4
--with-gnu-ld=yes
--without-libintl-prefix
--disable-option-checking
--disable-assembler
--disable-xzdec
--disable-doc
--disable-silent-rules
--disable-nls
