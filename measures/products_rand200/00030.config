--enable-debug
--enable-encoders=lzma1,delta
--enable-decoders=lzma1,armthumb
--enable-match-finders=hc3
--enable-checks=crc32,sha256
--enable-threads=yes
--enable-sandbox=auto
--enable-dependency-tracking
--enable-unaligned-access
--enable-fast-install=no
--with-aix-soname=both
--with-gnu-ld
--with-gnu-ld=yes
--without-libintl-prefix
--disable-lzmadec
--disable-lzmainfo
--disable-lzma-links
--disable-silent-rules
--disable-nls
--disable-rpath
