--enable-encoders=lzma1,lzma2
--enable-decoders=lzma1,lzma2
--enable-match-finders=bt3
--enable-checks=crc32,sha256
--enable-small
--enable-threads=no
--enable-assume-ram=128
--enable-symbol-versions
--enable-sandbox=capsicum
--enable-dependency-tracking
--with-pic=non-PIC
--with-aix-soname=aix
--with-gnu-ld
--with-gnu-ld=yes
--without-libiconv-prefix
--without-libintl-prefix
--disable-option-checking
--disable-xzdec
--disable-lzmainfo
--disable-scripts
--disable-doc
--disable-largefile
