--enable-debug
--enable-encoders=lzma1,arm
--enable-decoders=lzma1,ia64
--enable-match-finders=bt2,bt4
--enable-checks=crc32,crc64,sha256
--enable-small
--enable-threads=yes
--enable-sandbox=no
--enable-silent-rules
--enable-fast-install=no
--with-pic=non-PIC
--with-gnu-ld
--without-libiconv-prefix
--without-libintl-prefix
--disable-assembler
--disable-lzmadec
--disable-lzma-links
--disable-silent-rules
--disable-dependency-tracking
--disable-nls
--disable-rpath
