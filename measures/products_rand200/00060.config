--enable-debug
--enable-encoders=lzma1,lzma2,x86,armthumb,sparc,arm,ia64,powerpc,delta
--enable-decoders=lzma1,arm
--enable-match-finders=hc3,bt2,bt3,bt4
--enable-checks=crc32
--enable-assume-ram=128
--enable-symbol-versions
--enable-sandbox=capsicum
--enable-unsafe-type-punning
--enable-fast-install=no
--with-aix-soname=svr4
--with-gnu-ld
--without-libiconv-prefix
--without-libintl-prefix
--disable-assembler
--disable-lzma-links
--disable-silent-rules
--disable-nls
--disable-rpath
