--enable-debug
--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1,lzma2
--enable-match-finders=bt2,bt3
--enable-checks=crc32,sha256
--enable-small
--enable-threads=posix
--enable-symbol-versions
--enable-sandbox=no
--enable-dependency-tracking
--enable-unsafe-type-punning
--with-pic=PIC
--with-aix-soname=aix
--without-libintl-prefix
--disable-option-checking
--disable-assembler
--disable-xzdec
--disable-lzma-links
--disable-scripts
--disable-libtool-lock
--disable-rpath
--disable-largefile
