--enable-encoders=lzma1,sparc
--enable-decoders=lzma1,armthumb
--enable-match-finders=bt4
--enable-checks=crc32,crc64,sha256
--enable-small
--enable-symbol-versions
--enable-unaligned-access
--enable-fast-install=no
--with-pic=non-PIC
--with-aix-soname=both
--with-gnu-ld
--with-gnu-ld=yes
--without-libintl-prefix
--disable-option-checking
--disable-lzmadec
--disable-lzma-links
--disable-scripts
--disable-silent-rules
--disable-nls
--disable-rpath
