--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1,armthumb
--enable-match-finders=hc4,hc3,bt2
--enable-checks=crc32,crc64,sha256
--enable-threads=yes
--enable-sandbox=no
--enable-dependency-tracking
