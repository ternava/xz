--enable-encoders=lzma1,ia64
--enable-decoders=lzma1,lzma2
--enable-match-finders=hc3,bt2,bt4
--enable-checks=crc32
--enable-small
--enable-threads=yes
--enable-symbol-versions
--enable-sandbox=no
--enable-silent-rules
