--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1,armthumb
--enable-match-finders=bt4
--enable-checks=crc32,sha256
--enable-small
--enable-sandbox=capsicum
--enable-dependency-tracking
--enable-unaligned-access
--with-pic=PIC
--without-libintl-prefix
--disable-option-checking
--disable-xzdec
--disable-lzma-links
--disable-silent-rules
--disable-dependency-tracking
--disable-nls
--disable-rpath
