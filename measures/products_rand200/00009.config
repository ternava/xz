--enable-debug
--enable-encoders=lzma1,arm
--enable-decoders=lzma1,powerpc
--enable-match-finders=hc4,hc3,bt3
--enable-checks=crc32,sha256
--enable-sandbox=auto
--enable-silent-rules
--enable-dependency-tracking
--enable-fast-install
--enable-unsafe-type-punning
--enable-fast-install=no
--with-pic=PIC
--with-aix-soname=aix
--with-gnu-ld
--disable-option-checking
--disable-assembler
--disable-lzmadec
--disable-lzma-links
--disable-libtool-lock
--disable-nls
