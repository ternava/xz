--enable-encoders=lzma1,arm
--enable-decoders=lzma1,sparc
--enable-match-finders=hc3,bt4
--enable-checks=crc32
--enable-threads=posix
--enable-sandbox=no
--enable-fast-install
--enable-unsafe-type-punning
--with-gnu-ld=yes
--without-libiconv-prefix
--disable-xzdec
--disable-doc
--disable-silent-rules
--disable-dependency-tracking
--disable-largefile
