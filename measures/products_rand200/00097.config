--enable-debug
--enable-encoders=lzma1,lzma2
--enable-decoders=lzma1,sparc
--enable-match-finders=hc4,bt2,bt3,bt4
--enable-checks=crc32
--enable-threads=posix
--enable-symbol-versions
--enable-sandbox=auto
--enable-silent-rules
--enable-dependency-tracking
--enable-fast-install=no
--with-pic=PIC
--with-aix-soname=both
--with-gnu-ld
--with-gnu-ld=yes
--without-libiconv-prefix
--disable-assembler
--disable-lzmainfo
--disable-scripts
--disable-doc
--disable-dependency-tracking
