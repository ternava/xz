--enable-debug
--enable-encoders=lzma1
--enable-decoders=lzma1,x86
--enable-match-finders=hc4,bt4
--enable-checks=crc32,crc64,sha256
--enable-small
--enable-threads=no
--enable-assume-ram=128
--enable-sandbox=capsicum
--enable-silent-rules
--enable-dependency-tracking
--enable-fast-install=no
--with-pic=PIC
--with-aix-soname=both
--without-libiconv-prefix
--without-libintl-prefix
--disable-option-checking
--disable-lzmadec
--disable-lzma-links
--disable-doc
--disable-nls
