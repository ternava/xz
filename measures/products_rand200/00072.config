--enable-encoders=lzma1,delta
--enable-decoders=lzma1,powerpc
--enable-match-finders=hc4,hc3,bt2
--enable-checks=crc32,crc64
--enable-threads=yes
--enable-assume-ram=128
--enable-symbol-versions
--enable-sandbox=auto
--enable-silent-rules
--enable-fast-install=no
--with-aix-soname=svr4
--without-libiconv-prefix
--without-libintl-prefix
--disable-assembler
--disable-silent-rules
--disable-nls
--disable-largefile
