--enable-debug
--enable-encoders=lzma1,arm
--enable-decoders=lzma1
--enable-match-finders=hc4,bt2,bt3,bt4
--enable-checks=crc32,crc64,sha256
--enable-small
--enable-threads=posix
--enable-assume-ram=128
--enable-sandbox=auto
--enable-silent-rules
--enable-dependency-tracking
--enable-fast-install
--with-pic=non-PIC
--with-aix-soname=aix
--with-gnu-ld
--disable-option-checking
--disable-lzmadec
--disable-lzma-links
--disable-doc
--disable-silent-rules
--disable-nls
--disable-rpath
