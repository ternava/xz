--enable-debug
--enable-encoders=lzma1,armthumb
--enable-decoders=lzma1,lzma2,delta,x86,sparc,ia64,armthumb,arm,powerpc
--enable-match-finders=hc3,bt3
--enable-checks=crc32
--enable-small
--enable-threads=posix
--enable-symbol-versions
--enable-sandbox=auto
--enable-silent-rules
--enable-fast-install
--with-gnu-ld
--without-libiconv-prefix
--disable-lzma-links
--disable-scripts
--disable-silent-rules
