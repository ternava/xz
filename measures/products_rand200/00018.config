--enable-encoders=lzma1,lzma2,x86,armthumb,sparc,arm,ia64,powerpc,delta
--enable-decoders=lzma1,sparc
--enable-match-finders=hc4,hc3,bt3
--enable-checks=crc32,sha256
--enable-small
--enable-threads=yes
--enable-assume-ram=128
--enable-sandbox=no
--enable-fast-install
--enable-unaligned-access
--with-aix-soname=aix
--with-gnu-ld
--without-libiconv-prefix
--disable-option-checking
--disable-xzdec
--disable-doc
--disable-silent-rules
--disable-libtool-lock
--disable-nls
