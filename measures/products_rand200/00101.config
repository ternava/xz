--enable-debug
--enable-encoders=lzma1,sparc
--enable-decoders=lzma1,lzma2,delta,x86,sparc,ia64,armthumb,arm,powerpc
--enable-match-finders=hc4,bt2
--enable-checks=crc32
--enable-small
--enable-threads=no
--enable-assume-ram=128
--enable-sandbox=capsicum
--enable-fast-install
--with-pic=non-PIC
--with-aix-soname=both
--without-libiconv-prefix
--without-libintl-prefix
--disable-option-checking
--disable-assembler
--disable-xzdec
--disable-lzmadec
--disable-scripts
--disable-doc
--disable-libtool-lock
--disable-nls
--disable-rpath
