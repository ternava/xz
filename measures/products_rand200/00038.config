--enable-debug
--enable-encoders=lzma1,x86
--enable-decoders=lzma1,ia64
--enable-match-finders=hc3,bt2,bt4
--enable-checks=crc32,crc64
--enable-small
--enable-threads=no
--enable-assume-ram=128
--enable-symbol-versions
--enable-sandbox=auto
--enable-fast-install
--enable-unaligned-access
--enable-unsafe-type-punning
--with-aix-soname=both
--without-libintl-prefix
--disable-xzdec
--disable-lzmainfo
--disable-doc
--disable-dependency-tracking
--disable-rpath
