--enable-debug
--enable-encoders=lzma1,arm
--enable-decoders=lzma1
--enable-match-finders=bt2
--enable-checks=crc32,sha256
--enable-small
--enable-sandbox=auto
--enable-silent-rules
--enable-dependency-tracking
--with-pic=non-PIC
--with-aix-soname=aix
--with-gnu-ld
--without-libintl-prefix
--disable-assembler
--disable-xzdec
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-libtool-lock
--disable-nls
--disable-rpath
