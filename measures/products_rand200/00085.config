--enable-encoders=lzma1,x86
--enable-decoders=lzma1,ia64
--enable-match-finders=hc3,bt4
--enable-checks=crc32,sha256
--enable-threads=posix
--enable-assume-ram=128
--enable-sandbox=capsicum
--enable-dependency-tracking
--enable-fast-install
--enable-unaligned-access
--with-pic=PIC
--with-aix-soname=aix
--without-libiconv-prefix
--disable-option-checking
--disable-assembler
--disable-lzmainfo
--disable-scripts
--disable-dependency-tracking
--disable-libtool-lock
