--enable-encoders=lzma1,ia64
--enable-decoders=lzma1,ia64
--enable-match-finders=hc3
--enable-checks=crc32,sha256
--enable-threads=yes
--enable-assume-ram=128
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=auto
--enable-silent-rules
--enable-dependency-tracking
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--with-aix-soname=both
--without-libiconv-prefix
--without-libintl-prefix
--disable-xzdec
--disable-doc
--disable-dependency-tracking
--disable-libtool-lock
--disable-rpath
--disable-largefile
