--enable-encoders=lzma1,arm
--enable-decoders=lzma1,lzma2,delta,x86,sparc,ia64,armthumb,arm,powerpc
--enable-match-finders=hc3,bt2,bt4
--enable-checks=crc32
--enable-threads=yes
--enable-assume-ram=128
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=no
--enable-fast-install
--enable-unaligned-access
--enable-unsafe-type-punning
--with-pic=non-PIC
--with-gnu-ld
--with-gnu-ld=yes
--disable-assembler
--disable-xzdec
--disable-lzmainfo
--disable-lzma-links
--disable-dependency-tracking
--disable-rpath
--disable-largefile
