--enable-encoders=lzma1,armthumb
--enable-decoders=lzma1
--enable-match-finders=hc3,bt3,bt4
--enable-checks=crc32,sha256
--enable-small
--enable-threads=yes
--enable-assume-ram=128
--enable-shared=no
--enable-symbol-versions
--enable-sandbox=auto
--enable-silent-rules
--enable-static
--with-pic=non-PIC
--with-aix-soname=both
--with-gnu-ld=yes
--disable-option-checking
--disable-lzmainfo
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-silent-rules
--disable-libtool-lock
--disable-nls
--disable-rpath
