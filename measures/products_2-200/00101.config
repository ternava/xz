--enable-encoders=lzma1,arm
--enable-decoders=lzma1,lzma2
--enable-match-finders=bt3
--enable-checks=crc32
--enable-threads=no
--enable-shared=no
--enable-dependency-tracking
--enable-fast-install
--with-pic=non-PIC
--with-aix-soname=svr4
--with-gnu-ld=yes
--with-libiconv-prefix
--with-libintl-prefix
--disable-option-checking
--disable-assembler
--disable-xzdec
--disable-lzmadec
--disable-lzmainfo
--disable-lzma-links
--disable-scripts
--disable-silent-rules
--disable-libtool-lock
--disable-nls
--disable-rpath
