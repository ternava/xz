--enable-encoders=lzma1,x86
--enable-decoders=lzma1,lzma2,delta,x86,sparc,ia64,armthumb,arm,powerpc
--enable-match-finders=hc3
--enable-checks=crc32,crc64,sha256
--enable-dependency-tracking
--enable-fast-install=no
--with-pic=PIC
--with-aix-soname=both
--with-gnu-ld=yes
--with-libiconv-prefix
--disable-xzdec
--disable-lzmadec
--disable-lzmainfo
--disable-lzma-links
--disable-doc
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
--disable-rpath
