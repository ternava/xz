--enable-encoders=lzma1,powerpc
--enable-decoders=lzma1,x86
--enable-match-finders=hc4,bt4
--enable-checks=crc32,crc64,sha256
--enable-small
--enable-threads=yes
--enable-sandbox=capsicum
--enable-fast-install=no
--with-aix-soname=svr4
--with-gnu-ld
--with-gnu-ld=yes
--without-libintl-prefix
--disable-option-checking
--disable-lzmainfo
--disable-doc
--disable-silent-rules
--disable-nls
--disable-rpath
--disable-largefile
