--enable-encoders=lzma1,lzma2,x86,armthumb,sparc,arm,ia64,powerpc,delta
--enable-decoders=lzma1,x86
--enable-match-finders=hc4,bt2,bt3,bt4
--enable-checks=crc32
--enable-small
--enable-threads=posix
--enable-symbol-versions
--enable-sandbox=no
--enable-fast-install
--enable-unsafe-type-punning
--enable-static
--with-aix-soname=both
--with-gnu-ld=yes
--disable-assembler
--disable-xzdec
--disable-scripts
--disable-doc
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
--disable-rpath
