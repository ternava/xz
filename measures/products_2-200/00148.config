--enable-encoders=lzma1,lzma2,x86,armthumb,sparc,arm,ia64,powerpc,delta
--enable-decoders=lzma1
--enable-match-finders=hc3,bt2
--enable-checks=crc32,crc64
--enable-small
--enable-threads=yes
--enable-sandbox=no
--enable-silent-rules
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-static
--enable-fast-install=no
--with-pic=PIC
--with-aix-soname=svr4
--with-libintl-prefix
--disable-option-checking
--disable-doc
--disable-silent-rules
--disable-nls
--disable-rpath
--disable-largefile
