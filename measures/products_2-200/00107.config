--enable-encoders=lzma1,ia64
--enable-decoders=lzma1,x86
--enable-match-finders=hc3,bt3
--enable-checks=crc32
--enable-small
--enable-threads=yes
--enable-symbol-versions
--enable-sandbox=auto
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-static
--with-pic=non-PIC
--with-aix-soname=both
--without-libiconv-prefix
--without-libintl-prefix
--disable-assembler
--disable-lzmadec
--disable-lzma-links
--disable-scripts
--disable-silent-rules
--disable-dependency-tracking
--disable-libtool-lock
--disable-nls
--disable-largefile
