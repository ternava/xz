--enable-encoders=lzma1,armthumb
--enable-decoders=lzma1
--enable-match-finders=bt3
--enable-checks=crc32
--enable-threads=no
--enable-shared=no
--enable-sandbox=no
--enable-silent-rules
--enable-fast-install
--with-aix-soname=both
--with-gnu-ld=yes
--disable-option-checking
--disable-assembler
--disable-xzdec
--disable-lzma-links
--disable-silent-rules
--disable-dependency-tracking
--disable-nls
--disable-rpath
--disable-largefile
