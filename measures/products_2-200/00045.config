--enable-encoders=lzma1,x86
--enable-decoders=lzma1,armthumb
--enable-match-finders=hc4,bt3
--enable-checks=crc32,sha256
--enable-threads=no
--enable-assume-ram=128
--enable-symbol-versions
--enable-sandbox=capsicum
--enable-silent-rules
--enable-dependency-tracking
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-fast-install=no
--with-pic=non-PIC
--with-aix-soname=svr4
--with-gnu-ld=yes
--without-libiconv-prefix
--with-libintl-prefix
--disable-lzmainfo
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-rpath
