--enable-encoders=lzma1
--enable-decoders=lzma1,x86
--enable-match-finders=bt2
--enable-checks=crc32
--enable-small
--enable-threads=posix
--enable-assume-ram=128
--enable-shared=no
--enable-sandbox=auto
--enable-silent-rules
--enable-dependency-tracking
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-static
--with-pic=PIC
--with-aix-soname=svr4
--with-gnu-ld
--without-libiconv-prefix
--disable-assembler
--disable-lzma-links
--disable-scripts
--disable-silent-rules
--disable-dependency-tracking
--disable-libtool-lock
--disable-rpath
