--enable-encoders=lzma1,sparc
--enable-decoders=lzma1,lzma2
--enable-match-finders=hc3,bt2
--enable-checks=crc32,sha256
--enable-threads=yes
--enable-symbol-versions
--enable-sandbox=capsicum
--enable-fast-install
--enable-static
--with-aix-soname=svr4
--with-gnu-ld=yes
--with-libiconv-prefix
--without-libintl-prefix
--disable-option-checking
--disable-assembler
--disable-xzdec
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-libtool-lock
--disable-nls
--disable-largefile
