--enable-encoders=lzma1,delta
--enable-decoders=lzma1,x86
--enable-match-finders=hc3,bt4
--enable-checks=crc32
--enable-threads=yes
--enable-assume-ram=128
--enable-symbol-versions
--enable-sandbox=auto
--enable-silent-rules
--enable-dependency-tracking
--enable-static
--with-pic=non-PIC
--with-aix-soname=svr4
--with-libiconv-prefix
--disable-option-checking
--disable-xzdec
--disable-lzmainfo
--disable-silent-rules
--disable-dependency-tracking
--disable-libtool-lock
--disable-rpath
