--enable-encoders=lzma1,armthumb
--enable-decoders=lzma1,delta
--enable-match-finders=hc3,bt4
--enable-checks=crc32,crc64,sha256
--enable-small
--enable-shared=no
--enable-silent-rules
--enable-unaligned-access
--enable-unsafe-type-punning
--enable-static
--with-pic=non-PIC
--with-aix-soname=aix
--with-gnu-ld
--without-libiconv-prefix
--disable-option-checking
--disable-assembler
--disable-xzdec
--disable-lzma-links
--disable-scripts
--disable-doc
--disable-silent-rules
--disable-nls
--disable-rpath
