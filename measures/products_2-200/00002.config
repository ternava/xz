--enable-encoders=lzma1,armthumb
--enable-decoders=lzma1,lzma2
--enable-match-finders=hc3,bt2,bt3,bt4
--enable-checks=crc32,crc64
--enable-threads=no
--enable-symbol-versions
--enable-sandbox=no
--enable-silent-rules
--enable-dependency-tracking
--enable-unaligned-access
--with-pic=PIC
--with-aix-soname=svr4
--with-gnu-ld=yes
--with-libintl-prefix
--disable-xzdec
--disable-lzmadec
--disable-lzma-links
--disable-scripts
--disable-silent-rules
--disable-libtool-lock
