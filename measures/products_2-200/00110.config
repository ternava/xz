--enable-encoders=lzma1,lzma2
--enable-decoders=lzma1,powerpc
--enable-match-finders=hc4,bt2,bt4
--enable-checks=crc32
--enable-small
--enable-threads=posix
--enable-assume-ram=128
--enable-sandbox=auto
--enable-silent-rules
--enable-dependency-tracking
--enable-fast-install=no
--with-pic=PIC
--disable-option-checking
--disable-xzdec
--disable-lzma-links
--disable-scripts
--disable-silent-rules
--disable-nls
--disable-rpath
